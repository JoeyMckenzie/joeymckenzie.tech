{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": ["artisan", "boost:mcp"]
        },
        "herd": {
            "command": "php",
            "args": ["/Applications/Herd.app/Contents/Resources/herd-mcp.phar"],
            "env": {
                "SITE_PATH": "/Users/jmckenzie/github.com/joeymckenzie/joeymckenzie.tech"
            }
        },
        "serena": {
            "command": "uvx",
            "args": [
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server",
                "--context",
                "ide-assistant",
                "--project",
                "${PWD}",
                "--enable-web-dashboard",
                "false"
            ]
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp",
                "--api-key",
                "ctx7sk-af6cf6b0-1316-4b07-b635-a2b3720c96f6"
            ]
        }
    }
}
